
#include <bits/stdc++.h>
using namespace std;
int main()
{
    // declaration of array 
    // take input 
    cout<<"enter the value of n";
    int n;
    
    cin>>n;
    int a[n];
    
    //taking values in array
    for(int i=0;i<n;i++)
    {
        cout<<"enter value ";
        cin>>a[i];
        cout<<endl;
    }
    // printing array 
    
    for(int i=0;i<n;i++)
    {
    cout<<a[i]<<" ";    
        }
    
    // finding maxing in the array 
    int max=INT_MIN; 
    
    for(int i=0;i<n;i++)
    {
    if(max<a[i])
    {
        max=a[i];
    }
    }
    // printing array 
    
    if(max==INT_MIN)
    cout<<"0";
    else
    cout<<"max "<<max;
     
    return 0;
}
